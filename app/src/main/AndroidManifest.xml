<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.welop.svlit.mbank">

    <application
        android:name="com.welop.mbank.MBank"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.welop.mbank.activities.LobbyActivity" />
        <activity android:name="com.welop.mbank.activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.welop.mbank.activities.CreateLobbyActivity"
            android:parentActivityName="com.welop.mbank.activities.MainActivity" />
        <activity
            android:name="com.welop.mbank.activities.SignInActivity"
            android:exported="true"
            android:label="@string/title_activity_sign_in"
            android:theme="@style/AppTheme" />
        <activity android:name="com.welop.mbank.activities.SignUpActivity" />
        <activity
            android:name="com.welop.mbank.activities.StartActivity"
            android:exported="true" />
        <activity android:name="com.welop.mbank.activities.RestoreActivity" />
        <activity android:name="com.welop.mbank.activities.JoinLobbyActivity"></activity>
    </application>

</manifest>